<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             android:layout_width="match_parent"
             android:layout_height="40dp"
             android:background="@drawable/select_temple1_table_title_name_bg"
             android:orientation="horizontal">

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingLeft="10dp"
        android:paddingRight="10dp">

        <ImageView
            android:id="@+id/img_tableName_dot"
            android:layout_width="@dimen/space_default_large_s"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_marginRight="5dp"
            android:scaleType="centerInside"
            android:src="@drawable/graph_blackpot"
            android:visibility="gone"/>

        <TextView
            android:id="@+id/tv_tableName_value"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:ellipsize="end"
            android:gravity="center_vertical"
            android:maxLines="2"
            android:textSize="@dimen/font_small"/>
        <!--style="@style/tv_unit_chart_m_three"-->
    </TableRow>
</FrameLayout>
